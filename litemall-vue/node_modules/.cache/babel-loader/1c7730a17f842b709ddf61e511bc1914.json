nfo = data.expressInfo;\n      });\n    }\n  },\n\n  components: {\n    [Dialog.name]: Dialog,\n    [CellGroup.name]: CellGroup,\n    [Cell.name]: Cell,\n    [Button.name]: Button,\n    [SubmitBar.name]: SubmitBar,\n    [Card.name]: Card,\n    [Field.name]: Field\n  }\n};\n</script>\n\n\n<style lang=\"scss\" scoped>\n.order_detail {\n  padding-bottom: 70px;\n}\n</style>\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;AAwFA,IAAAA,IAAA,GAAAC,OAAA;AAMA,IAAAC,QAAA,GAAAC,OAAA,CAAAC,OAAA,GAEA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,QAAA;MACAC,UAAA;MACAC,SAAA;MACAC,UAAA;MACAC,YAAA;MACAC,WAAA;IACA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA,KAAAC,IAAA;EACA;EAEAC,OAAA;IACAC,OAAA,WAAAA,QAAA;MACA,WAAAC,KAAA,CAAAZ,OAAA,OAAAI,SAAA;IACA;IACAS,WAAA,WAAAA,YAAAC,EAAA;MAAA,IAAAC,KAAA;MACA,IAAAC,IAAA;MACA,KAAAC,OAAA,CACAC,OAAA;QAAAC,OAAA;MAAA,GACAC,IAAA;QACA,IAAAC,gBAAA;UAAAC,OAAA,EAAAR;QAAA,GAAAM,IAAA;UACAL,KAAA,CAAAQ,MAAA;UACAR,KAAA,CAAAS,OAAA,CAAAC,EAAA;QACA;MACA,GACAC,KAAA;IACA;IACAC,WAAA,WAAAA,YAAAb,EAAA;MAAA,IAAAc,MAAA;MACA,KAAAX,OAAA,CACAC,OAAA;QAAAC,OAAA;MAAA,GACAC,IAAA;QACA,IAAAC,gBAAA;UAAAC,OAAA,EAAAR;QAAA,GAAAM,IAAA;UACAQ,MAAA,CAAAnB,IAAA;UACAmB,MAAA,CAAAL,MAAA;QACA;MACA,GACAG,KAAA;IACA;IACAG,YAAA,WAAAA,aAAAf,EAAA;MAAA,IAAAgB,MAAA;MACA,KAAAb,OAAA,CACAC,OAAA;QACAC,OAAA;MACA,GACAC,IAAA;QACA,IAAAW,iBAAA;UAAAT,OAAA,EAAAR;QAAA,GAAAM,IAAA;UACAU,MAAA,CAAArB,IAAA;UACAqB,MAAA,CAAAP,MAAA;QACA;MACA,GACAG,KAAA;IACA;IACAM,WAAA,WAAAA,YAAAlB,EAAA;MAAA,IAAAmB,MAAA;MACA,KAAAhB,OAAA,CACAC,OAAA;QAAAC,OAAA;MAAA,GACAC,IAAA;QACA,IAAAc,gBAAA;UAAAZ,OAAA,EAAAR;QAAA,GAAAM,IAAA;UACAa,MAAA,CAAAxB,IAAA;UACAwB,MAAA,CAAAV,MAAA;QACA;MACA,GACAG,KAAA;IACA;IACAS,YAAA,WAAAA,aAAArB,EAAA;IACAsB,KAAA,WAAAA,MAAAtB,EAAA;MACA,KAAAU,OAAA,CAAAa,IAAA;QAAAC,IAAA;QAAAC,MAAA;UAAAjB,OAAA,EAAAR;QAAA;MAAA;IACA;IACAL,IAAA,WAAAA,KAAA;MAAA,IAAA+B,MAAA;MACA,IAAAlB,OAAA,QAAAmB,MAAA,CAAAC,KAAA,CAAApB,OAAA;MACA,IAAAqB,gBAAA;QAAArB,OAAA,EAAAA;MAAA,GAAAF,IAAA,WAAAwB,GAAA;QACA,IAAA3C,IAAA,GAAA2C,GAAA,CAAA3C,IAAA,CAAAA,IAAA;QACAuC,MAAA,CAAApC,SAAA,GAAAH,IAAA,CA